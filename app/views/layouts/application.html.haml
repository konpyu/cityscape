!!! 5
%html
  %head
    %title
      Pariscape
    = stylesheet_link_tag    'application', media: 'all'
    = javascript_include_tag 'application'
    = csrf_meta_tags
  %body
    .container
      .row
        .col-md-12
          %h4.space-bottom-1h
            = "Pariscape"
            %span.space-left-1
              = link_to 'about', "/about"
            %span.pull-right
              = "#{@photo_count} Pics | Last Update: 2014-11-10"

      = yield
